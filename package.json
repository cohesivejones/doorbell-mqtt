    "rimraf": "^2.6.2",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^14.14.31",
    "@types/sequelize": "^4.28.9",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
